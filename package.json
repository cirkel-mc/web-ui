{
  "name": "cirkel-web-ui",
  "description": "UIKit for Cirkel Web",
  "private": false,
  "version": "0.1.6",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "tsc && vite build",
    "build:watch": "rm -rf ./dist && tsc && vite build --watch",
    "lint": "eslint --fix './lib/**/*.{ts,tsx,js,jsx}' && prettier . --write './lib/**/*.{ts,tsx,js,jsx,css}'",
    "preview": "vite preview",
    "prepare": "husky",
    "generate-svg": "npx @svgr/cli --out-dir assets/svg-components assets"
  },
  "lint-staged": {
    "./lib/**/*.{ts,tsx,js,jsx}": "eslint --fix",
    "./lib/**/*.{ts,tsx,js,jsx,css}": "prettier --write"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-image-presets": "^0.3.4",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.1",
    "@tanstack/react-table": "^8.19.2",
    "antd": "^5.20.1",
    "cva": "npm:class-variance-authority",
    "dayjs": "^1.11.11",
    "iconsax-react": "^0.0.8",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.2.1",
    "react-spinners": "^0.14.1",
    "react-toastify": "^9.1.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.3.6",
    "uuid": "^9.0.1"
  }
}
